@model DataResult<NewsDto>

@{
    ViewData["Title"] = "Details";
}
<div class=" card container my-5">
	<div class="row">
		<div class="col-8 offset-2 card-body">
			<h4># @Model.Data.News.Id News Detail</h4>
			<hr />
			
					<div class="form-group">
			        	<b>Title :</b>
						@Model.Data.News.Title
					</div>
					<div class="form-group">
						<b>Subtitle :</b>
						@Model.Data.News.SubTitle
					</div>
					<div class="form-group">
						<b>Content :</b>
						@Model.Data.News.Content
					</div>
			

			<div class="mb-3">
				<a class="btn btn-primary"asp-action="Index">Close</a>
			</div>
		</div>
	</div>
</div>

